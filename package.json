{
  "name": "@codai/memorai-service",
  "version": "2.1.0",
  "description": "Production-Ready Agent-Native Memory Control Protocol",
  "private": true,
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "build": "cross-env TEMP_OVERRIDE=\"E:\\GitHub\\codai-project\\temp\" TMP_OVERRIDE=\"E:\\GitHub\\codai-project\\temp\" turbo build",
    "build:all": "turbo build",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "clean": "turbo clean && rm -rf node_modules",
    "publish-packages": "node scripts/publish-packages.js",
    "production-only": "echo 'PRODUCTION-ONLY MODE: Use published packages and MCP tools only. No local development allowed.'",
    "dev": "turbo dev",
    "test": "turbo test",
    "test:watch": "turbo test:watch",
    "test:ui": "turbo test:ui",
    "test:run": "turbo test:run",
    "test:coverage": "turbo test:coverage",
    "test:e2e": "echo 'Use MCP tests instead: npm run test:mcp'",
    "test:e2e:ui": "echo 'Use MCP browser tests through VS Code'",
    "test:e2e:headed": "echo 'Use MCP browser tests through VS Code'",
    "test:e2e:debug": "echo 'Use MCP browser tests through VS Code'",
    "test:e2e:enterprise": "echo 'Use: npm run test:mcp'",
    "test:e2e:performance": "echo 'Use: npm run test:mcp'",
    "test:e2e:security": "echo 'Use: npm run test:mcp'",
    "test:e2e:load": "echo 'Use: npm run test:mcp'",
    "test:mcp": "node tests/mcp/run-mcp-tests.js",
    "test:mcp:quick": "node test-quick-validation.js",
    "test:enterprise": "node tests/mcp/run-mcp-tests.js",
    "test:api": "node test-quick-validation.js",
    "start:dashboard": "turbo start --filter=@codai/memorai-dashboard",
    "start:api": "turbo start --filter=@codai/memorai-api",
    "start:mcp": "turbo start --filter=@codai/memorai-mcp"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^16.1.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.12.12",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "@vitest/coverage-v8": "^2.1.9",
    "@vitest/ui": "^2.1.9",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.1",
    "happy-dom": "^12.0.0",
    "jsdom": "^23.0.0",
    "postcss": "^8.4.38",
    "prettier": "^3.3.3",
    "prisma": "^5.12.1",
    "tailwindcss": "^3.4.3",
    "turbo": "^2.5.4",
    "typescript": "^5.5.4",
    "vitest": "^2.1.9"
  },
  "packageManager": "pnpm@8.15.6",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/codai-ecosystem/memorai.git"
  },
  "keywords": [
    "memory",
    "ai",
    "mcp",
    "agents",
    "semantic-search",
    "vector-database"
  ],
  "license": "MIT",
  "author": "Dragos CV",
  "dependencies": {
    "@auth/prisma-adapter": "^1.6.0",
    "@heroicons/react": "^2.0.18",
    "@prisma/client": "^5.12.1",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@types/ioredis": "^5.0.0",
    "@types/pg": "^8.15.4",
    "@types/sqlite3": "^3.1.11",
    "@types/uuid": "^9.0.8",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.263.1",
    "next-auth": "^4.24.7",
    "pg": "^8.16.3",
    "playwright": "^1.53.2",
    "postgres": "^3.4.4",
    "recharts": "^2.12.7",
    "sqlite3": "^5.1.6",
    "tailwind-merge": "^2.3.0",
    "uuid": "^9.0.1",
    "zod": "^3.23.8"
  },
  "type": "module"
}
