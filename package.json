{
  "name": "@codai/memorai-service",
  "version": "2.1.0",
  "description": "Production-Ready Agent-Native Memory Control Protocol",
  "private": true,
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "build": "next build",
    "lint": "next lint --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "clean": "turbo clean && rm -rf node_modules",
    "publish-packages": "node scripts/publish-packages.js",
    "dev": "next dev -p 4002",
    "test": "turbo test",
    "test:watch": "turbo test:watch",
    "test:ui": "turbo test:ui",
    "test:run": "turbo test:run",
    "test:coverage": "turbo test:coverage",
    "test:mcp": "node tests/mcp/run-mcp-tests.js",
    "start:dashboard": "turbo start --filter=@codai/memorai-dashboard",
    "start:api": "turbo start --filter=@codai/memorai-api",
    "start:mcp": "turbo start --filter=@codai/memorai-mcp",
    "start": "next start -p 4002",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@playwright/test": "^1.49.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.12.12",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.19",
    "chalk": "^5.3.0",
    "eslint": "^8.57.1",
    "glob": "^11.0.3",
    "happy-dom": "^18.0.1",
    "jsdom": "^23.0.0",
    "postcss": "^8.4.38",
    "prettier": "^3.3.3",
    "prisma": "^5.12.1",
    "tailwindcss": "^3.4.3",
    "turbo": "^2.5.4",
    "typescript": "^5.5.4",
    "vitest": "^3.2.4",
    "ws": "^8.18.3"
  },
  "packageManager": "pnpm@8.15.6",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/codai-ecosystem/memorai.git"
  },
  "keywords": [
    "memory",
    "ai",
    "mcp",
    "agents",
    "semantic-search",
    "vector-database"
  ],
  "license": "MIT",
  "author": "Dragos CV",
  "dependencies": {
    "@auth/prisma-adapter": "^1.6.0",
    "@prisma/client": "^5.12.1",
    "@qdrant/js-client-rest": "1.7.0",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@types/pg": "^8.15.4",
    "@types/sqlite3": "^3.1.11",
    "@types/uuid": "^9.0.8",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "ioredis": "^5.6.1",
    "lucide-react": "^0.263.1",
    "next": "15.3.3",
    "next-auth": "^4.24.7",
    "openai": "^4.20.0",
    "pg": "^8.16.3",
    "playwright": "^1.53.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "tailwind-merge": "^2.3.0",
    "zod": "^3.23.8"
  },
  "type": "module"
}